{
  "hosting": {
    "public": "build",
    "cleanUrls": true,
    "headers": [ {
      "source" : "**",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "public, max-age=1800"
      } ]
    }, {
      "source" : "/scripts/**/*",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "public, max-age=31536000"
      } ]
    }, {
      "source" : "/images/**/*",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "public, max-age=31536000"
      } ]
    }, {
      "source" : "/assets/**/*",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "public, max-age=31536000"
      } ]
    } ],
    "redirects": [ {
      "source": "/default.aspx?epslanguage=sv",
      "destination": "/",
      "type": 301
    }, {
      "source": "/templates/@(p|P)age____34.aspx*",
      "destination": "/",
      "type": 301
    }, {
      "source": "/templates/@(p|P)age____123.aspx*",
      "destination": "/program",
      "type": 301
    }, {
      "source": "/templates/@(p|P)age____476.aspx*",
      "destination": "/kontakt",
      "type": 301
    }, {
      "source": "/templates/@(p|P)age____127.aspx*",
      "destination": "/om-skolan/lasarsdata",
      "type": 301
    }, {
      "source": "/templates/@(p|P)age____5.aspx*",
      "destination": "/om-skolan",
      "type": 301
    }, {
      "source": "/templates/@(p|P)age____772.aspx*",
      "destination": "/program/matematik-spets",
      "type": 301
    }, {
      "source": "/templates/@(p|P)age____710.aspx*",
      "destination": "/program/ekonomi",
      "type": 301
    } ]
  }
}
