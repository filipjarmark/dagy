{
  "hosting": {
    "public": "build",
    "cleanUrls": true,
    "headers": [ {
      "source" : "**",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "public, max-age=1800"
      } ]
    }, {
      "source" : "/images/**/*",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "public, max-age=31536000"
      } ]
    }, {
      "source" : "/assets/**/*",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "public, max-age=31536000"
      } ]
    } ]
  }
}
